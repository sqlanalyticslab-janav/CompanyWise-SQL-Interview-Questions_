(8315, 1002, '2022-08-05', 80052, 150.00),
(8236, 1003, '2022-06-18', 70009, 220.00),
(8252, 1004, '2022-05-15', 80952, 180.00),
(8301, 1005, '2022-11-05', 80882, 250.00);


