CREATE TABLE Sales (
    sales_id INT PRIMARY KEY,
    customer_id VARCHAR(10),
    product_id VARCHAR(10),
    product_type VARCHAR(20),
    sales_year INT CHECK (sales_year >= 1900),
    sales_region VARCHAR(50),
    units_sold INT CHECK (units_sold >= 0)
);

